{% extends 'base.html.twig' %}

{% block title %}LivAddress index{% endblock %}

{% block body %}
    <h1>LivAddress index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FirstNameLiv</th>
                <th>LastNameLiv</th>
                <th>FirstAdLiv</th>
                <th>SecondAdLiv</th>
                <th>ZipcodeLiv</th>
                <th>TownLiv</th>
                <th>CountryLiv</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for liv_address in liv_addresses %}
            <tr>
                <td>{{ liv_address.id }}</td>
                <td>{{ liv_address.firstNameLiv }}</td>
                <td>{{ liv_address.lastNameLiv }}</td>
                <td>{{ liv_address.firstAdLiv }}</td>
                <td>{{ liv_address.secondAdLiv }}</td>
                <td>{{ liv_address.zipcodeLiv }}</td>
                <td>{{ liv_address.townLiv }}</td>
                <td>{{ liv_address.countryLiv }}</td>
                <td>
                    <a href="{{ path('liv_address_show', {'id': liv_address.id}) }}">show</a>
                    <a href="{{ path('liv_address_edit', {'id': liv_address.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('liv_address_new') }}">Create new</a>
{% endblock %}
